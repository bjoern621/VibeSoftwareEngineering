meta {
  name: 06 - Get Booking Not Found
  type: http
  seq: 6
}

get {
  url: {{baseUrl}}/api/buchungen/99999
  body: none
  auth: bearer
}

auth:bearer {
  token: {{authToken}}
}

script:pre-request {
  const axios = require('axios');
  const baseUrl = bru.getEnvVar("baseUrl");
  
  try {
    await axios.post(`${baseUrl}/api/kunden/registrierung`, {
      email: "bruno.test@example.com",
      password: "SecurePass123!",
      firstName: "Bruno",
      lastName: "Testuser",
      phoneNumber: "+49 30 12345678",
      street: "Teststra√üe 123",
      postalCode: "10115",
      city: "Berlin",
      driverLicenseNumber: "B1234567890"
    });
  } catch (e) {}
  
  try {
    const loginRes = await axios.post(`${baseUrl}/api/kunden/login`, {
      email: "bruno.test@example.com",
      password: "SecurePass123!"
    });
    if (loginRes.data && loginRes.data.token) {
      bru.setEnvVar("authToken", loginRes.data.token);
    }
  } catch (e) {}
}

tests {
