meta {
  name: 05 - Search Vehicles With Type
  type: http
  seq: 5
}

get {
  url: {{baseUrl}}/api/fahrzeuge/suche?von=2025-12-01&bis=2025-12-10&typ=COMPACT_CAR
  body: none
  auth: none
}

tests {
  test("Status is 200 OK", function() {
    expect(res.status).to.equal(200);
  });

  test("Response is an array", function() {
    expect(res.body).to.be.an("array");
  });

  test("All vehicles are COMPACT_CAR if any returned", function() {
    if (res.body.length > 0) {
      res.body.forEach(vehicle => {
        expect(vehicle.vehicleType).to.equal("COMPACT_CAR");
      });
    }
  });
}
