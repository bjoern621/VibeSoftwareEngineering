delete {
  url: {{baseUrl}}/api/reservations/1
  body: none
  auth: bearer
}

auth:bearer {
  token: {{authToken}}
}

tests {
  test("Status code is 204 or 404", function() {
    expect([204, 404]).to.include(res.status);
  });
  
  test("If 204: No content in response", function() {
    if (res.status === 204) {
      // 204 responses have empty body (undefined or empty string)
      expect(res.body === undefined || res.body === '').to.be.true;
    }
  });
}
