tests {
  test("Status should be 200 OK", function() {
    expect(res.getStatus()).to.equal(200);
  });
  
  test("Response should be JSON", function() {
    expect(res.getHeader('content-type')).to.contain('application/json');
  });
  
  test("Status should be SUBMITTED", function() {
    const data = res.getBody();
    expect(data.status).to.equal('SUBMITTED');
  });
  
  test("Should have submittedAt timestamp", function() {
    const data = res.getBody();
    expect(data.submittedAt).to.not.be.null;
  });
}

